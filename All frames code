    private void cmbchooseflightActionPerformed(java.awt.event.ActionEvent evt) {                                                
        // TODO add your handling code here:
    }                                               

    private void btntotalActionPerformed(java.awt.event.ActionEvent evt) {                                         
            if(cmbchooseflight.getSelectedItem()=="AirAsia")
            {
                  txttotal.setText("5000");
            }
             if(cmbchooseflight.getSelectedItem()=="IndiGo")
            {
                  txttotal.setText("6000");
            }
              if(cmbchooseflight.getSelectedItem()=="Air India")
            {
                  txttotal.setText("7000");
            }
              if(cmbchooseflight.getSelectedItem()=="Vistara")
            {
                  txttotal.setText("5500");
            }
              if(cmbchooseflight.getSelectedItem()=="Alliance Air ")
            {
                  txttotal.setText("9000");
            }
    }                                        

    private void btnaddActionPerformed(java.awt.event.ActionEvent evt) {                                       
      String id,chooseflight,source,destination,duration,accomodation,flight,ticket,total;
           id=txtid.getText();
        chooseflight =(String) cmbchooseflight.getSelectedItem();
         accomodation =(String) cmbaccomodation.getSelectedItem();
        flight =(String) cmbflight.getSelectedItem();
        ticket =(String) cmbticket.getSelectedItem();
          source=txtsource.getText();
          destination=txtdestination.getText();
          duration=txtduration.getText();
          total=txttotal.getText();
          Date d=(Date)datechooser.getDate();
          DateFormat dt=DateFormat.getDateInstance();
          String dstring=dt.format(d);
          
           String query="INSERT INTO `practice2`.`book_flight` (`id`, `chooseflight`, `source`, `destination`, `duration`, `date`,`accomodation`, `flight`, `ticket`,`total`) VALUES (' " + id+ " ',' "+ chooseflight+" ',' " +source+ " ',' " +destination+ " ',' " +duration+ " ',' " + dstring+ " ',' " +accomodation+ " ',' " +flight+ " ',' " +ticket+ " ',' " +total+ " ')";
       

           try
           {
               
            Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/practice2","Akshata","Akshata#11");
            Statement st=con.createStatement();
           st.executeUpdate(query);
           JOptionPane.showMessageDialog(this,"Records added sucessfully");
               
           }
           catch(Exception e)
           {
               JOptionPane.showMessageDialog(null,"error"+e);
           }
         
    }                                      

    private void btnpayActionPerformed(java.awt.event.ActionEvent evt) {                                       
        btnpay.setText("Paid");
        txtid.setText("  ");
        txtsource.setText("   ");
        txtdestination.setText(" ");
        txtduration.setText("  ");
        txttotal.setText("  ");
        cmbchooseflight.setSelectedItem("  ");
        cmbaccomodation.setSelectedItem("  ");
        cmbticket.setSelectedItem("  ");
        cmbflight.setSelectedItem("  ");
        datechooser.setDateFormatString("  ");
    }                                      

