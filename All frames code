    }                                        

    private void btnloginActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String username,password;
           username=txtuser.getText();
           password=txtpass.getText();
           String query="INSERT INTO `practice2`.`login` ( `username`, `password`) VALUES (' " + username+ " ',' "+password+" ')";
           try
           {
            Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/practice2","Akshata","Akshata#11");
            Statement st=con.createStatement();
           st.executeUpdate(query);
           JOptionPane.showMessageDialog(btnlogin,"Records added sucessfully");
               
           }
           catch(Exception e)
           {
               JOptionPane.showMessageDialog(null,"error"+e);
           }
           txtuser.setText(" ");
           txtpass.setText(" ");
    }                                        

