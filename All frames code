    private void btnaddActionPerformed(java.awt.event.ActionEvent evt) {                                       
      String id,fname,lname,address,mobileno,email,aadharno;
    
          id=txtid.getText();
           fname=txtfname.getText();
          lname=txtlname.getText();
          address=txtaddress.getText();
          mobileno=txtmobileno.getText();
          email=txtemail.getText();
          aadharno=txtaadharno.getText();
           String query="INSERT INTO `practice2`.`user detailes` (`id`,`fname`, `lname`, `address`, `mobileno`, `email`, `aadharno`) VALUES (' " + id+ " ',' " + fname+ " ',' "+ lname+" ',' " +address+ " ',' " + mobileno+ " ',' " +email+ " ',' " + aadharno+ " ')";
       

           try
           {
               
            Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/practice2","Akshata","Akshata#11");
            Statement st=con.createStatement();
           st.executeUpdate(query);
           JOptionPane.showMessageDialog(this,"Records added sucessfully");
               
           }
           catch(Exception e)
           {
               JOptionPane.showMessageDialog(null,"error"+e);
           }
           txtid.setText(" ");
            txtid.setText(" ");
           txtfname.setText(" ");
          txtlname.setText(" ");
          txtaddress.setText(" ");
         txtmobileno.setText(" ");
          txtemail.setText(" ");
          txtaadharno.setText(" ");
    }                                      

