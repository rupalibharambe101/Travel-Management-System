    private void btnreceiptActionPerformed(java.awt.event.ActionEvent evt) {                                           
        
        try
        {
            Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/practice2","Akshata","Akshata#11");
            Statement st=con.createStatement();
            Statement st1=con.createStatement();
            String query="SELECT * FROM `practice2`.`user detailes` WHERE (id="+Integer.parseInt(txtid.getText())+")";
            ResultSet rs;
            rs=st.executeQuery(query);
          
            if(rs.first())
            {
                String fname,lname,address,mobileno,email,total;
                fname=rs.getString("fname");
                lname=rs.getString("lname");
                address=rs.getString("address");
                mobileno=rs.getString("mobileno");
                email=rs.getString("email");
       
                 int num1;    
                String ref ="";        
                num1 = 1325 + (int) (Math.random()*4238);
                ref += num1 + 1325;
                txtreceipt.setText("        Travel Management System:\n\n"
            +"Ref:\t" + ref
            + "\n------------------------------------------------------"
            +"\nName:\t" + fname
            +"\nSurname:\t" + lname
            +"\nAddress:\t"+ address
            +"\nTelephone:\t"+ mobileno
            +"\nEmail:\t"+ email
            +"\nTotal:\t"+ 5000
            +"\n-------------------------------------------------------"
            +"\n\nThanks for Using \nTravel Management System"
        );
            }
         else
            {
                 JOptionPane.showMessageDialog(btnreceipt,"No Such Records Found");
        
            }
       
        }
        catch(Exception e)
        {
           
            JOptionPane.showMessageDialog(this," Error"+e);
            
        }
    }                                          

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
       this.dispose();
    }                                        

