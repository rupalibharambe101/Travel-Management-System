    private void btnsearchActionPerformed(java.awt.event.ActionEvent evt) {                                          
          
        String query="DELETE FROM `practice2`.`book_flight` WHERE (id="+Integer.parseInt(txtid.getText())+")";
        try
        {
            Connection con=DriverManager.getConnection("jdbc:mysql://localhost:3306/practice2","Akshata","Akshata#11");
            Statement st=con.createStatement();
           st.executeUpdate(query);
         
           JOptionPane.showMessageDialog(null,"Records deleted sucessfully");
         
        }
        catch(Exception e)
        {
           
            JOptionPane.showMessageDialog(null,"Connection Error");
            
        }
        txtid.setText(" ");
    }                                         

